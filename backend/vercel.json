{
  "version": 2,
  "builds": [
    {
      "src": "./app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/users/login",
      "dest": "/app.js"
    },
    {
      "src": "/users/register",
      "dest": "/app.js"
    },
    {
      "src": "/users/view",
      "dest": "/app.js"
    },
    {
      "src": "/users/remove",
      "dest": "/app.js"
    },
    {
      "src": "/request",
      "dest": "/app.js"
    },
    {
      "src": "/request/checkout/(.*)",
      "dest": "/app.js"
    },
    {
      "src": "/request/admin",
      "dest": "/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ]
}
